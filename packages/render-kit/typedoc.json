{
  "entryPointStrategy": "expand",
  "entryPoints": ["./src/lib"],
  "tsconfig": "tsconfig.lib.json",
  "compilerOptions": {},
  "exclude": [
    "**/*.(spec|test|e2e).ts",
    "docs/**",
    "tests/**",
    "specs/**",
    "spec/**",
    "test/**",
    "**/index.ts"
  ],
  "externalPattern": ["**/node_modules/**"],
  "excludeExternals": true,
  "excludeInternal": false,
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeNotDocumented": false,
  "externalSymbolLinkMappings": {},
  "includes": "",
  "out": "../../docs/packages/render-kit",
  "emit": "docs",
  "theme": "hierarchy",
  "name": "render-kit",
  "includeVersion": true,
  "readme": "./README.md",
  "disableSources": false,
  "excludeTags": [],
  "cname": "",
  "sourceLinkTemplate": "",
  "gitRevision": "main",
  "gitRemote": "origin",
  "htmlLang": "en",
  "githubPages": true,
  "gaID": "",
  "hideGenerator": true,
  "searchInComments": false,
  "cleanOutputDir": true,
  "titleLink": "",
  "navigationLinks": {
    "github": "https://github.com/permafacts/ar-kit", 
    "dapp": "https://facts.g8way.io",
    "twitter": "https://twitter.com/permafacts",
    "discord": "https://discord.com/invite/uGg8VAvqU7"
  },
  "sidebarLinks": {},
  "commentStyle": "all",
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [],
  "sort": ["visibility", "required-first", "source-order"],
  "visibilityFilters": {
    "protected": true,
    "private": true,
    "inherited": true,
    "external": true
  },
  "searchCategoryBoosts": {},
  "searchGroupBoosts": {},
  "preserveWatchOutput": false,
  "skipErrorChecking": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  },
  "requiredToBeDocumented": [],
  "treatWarningsAsErrors": false,
  "intentionallyNotExported": [],
  "logLevel": "Verbose",
  "plugin": ["typedoc-plugin-rename-defaults", "typedoc-theme-hierarchy"]
}
